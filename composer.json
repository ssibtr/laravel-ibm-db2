{
    "name": "guidofaecke/laravel-ibm-db2",
    "description": "laravel-ibm-db2 is a simple DB2 service provider for Laravel. It provides DB2 Connection by extending the Illuminate Database component of the laravel framework.",
    "keywords": [
        "laravel",
        "DB2",
        "Database",
        "PDO",
        "ODBC"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Rault",
            "role": "Developer"
        },
        {
            "name": "Guido Faecke",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "slevomat/coding-standard": "^8.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "GuidoFaecke\\DB2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GuidoFaecke\\DB2\\DB2ServiceProvider"
            ]
        }
    }
}
